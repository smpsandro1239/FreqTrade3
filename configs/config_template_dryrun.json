{
  "_comment": "FreqTrade3 - Dry Run Configuration",
  "_template": "Dry Run - Modo de Teste Seguro",
  "_description": "Configuração segura para testes sem dinheiro real",
  "_environment": "DRY_RUN",
  "_safety_level": "MAXIMUM",
  "_created": "2025-11-05",
  "_warning": "ESTA CONFIGURAÇÃO É 100% SEGURA - NÃO USA DINHEIRO REAL",
  "common": {
    "timeframe": "15m",
    "stake_currency": "USDT",
    "stake_amount": 100,
    "unfilledtimeout": {
      "entry": 30,
      "exit": 30
    },
    "startup_candles": 30
  },
  "dry_run": true,
  "dry_run_wallet": 10000.0,
  "max_open_trades": 3,
  "amend_last_stake_amount": true,
  "stoploss": -0.1,
  "trailing_stop": false,
